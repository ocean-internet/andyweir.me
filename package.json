  "scripts": {
    "clean": "gatsby clean",
    "build": "run-p build:**",
    "build:app": "npm run clean && gatsby build",
    "develop": "npm run clean && gatsby develop",
    "lint": "eslint './{.storybook,gatsby,lambda,src,static}/**/*.js' './gatsby-*.js' './optimise.js'",
