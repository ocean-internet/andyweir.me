    "start": "run-p start:**",
    "start:app": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "storybook": "NODE_ENV=production start-storybook -s ./public,./static",
    "build-storybook": "NODE_ENV=production build-storybook -s ./public,./static",
    "sonar": "npm run lint:report; npm run test; sonar-scanner"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml"
  }
